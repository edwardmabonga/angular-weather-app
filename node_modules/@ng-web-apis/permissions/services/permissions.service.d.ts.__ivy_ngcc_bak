import { Observable } from 'rxjs';
export declare type PermissionsQueryArgs = Parameters<typeof Permissions.prototype.query>[0];
export declare class PermissionsService {
    private readonly permissions;
    private permissionsSupported;
    constructor(permissions: Permissions, permissionsSupported: boolean);
    state(name: PermissionName): Observable<PermissionState>;
    state(descriptor: PermissionsQueryArgs): Observable<PermissionState>;
}
